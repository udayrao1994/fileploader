<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <style>
        table { border-collapse: collapse; width: 100%; }
        th, td { border: 1px solid #ddd; padding: 8px; }
        th { background-color: #f2f2f2; }
    </style>
</head>
<body>
    <h1><%= title %></h1>
    <table>
        <thead>
            <tr>
                <% headers.forEach(header => { %>
                    <th><%= header %></th>
                <% }); %>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(row => { %>
                <tr>
                    <% row.forEach(cell => { %>
                        <td><%= cell %></td>
                    <% }); %>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
